#DROP TABLE IF EXISTS `tscans`;
CREATE TABLE `tscans` (
        `user_name` varchar(30) default NULL,
        `det` varchar(30) default 'TP',
        `recoVer` varchar(10) default 'v6r2p1',
        `run` int(11) default NULL,
        `subrun` int(11) default NULL,
        `gate` int(11) default NULL,
        `slice` int(11) default NULL,
        `flags` int(10) unsigned default NULL,
        `track_prongs` tinyint(4) default NULL,
        `shower_prongs` tinyint(4) default NULL,
        `vertex_hits` tinyint(4) default NULL,
        `prong1_character` tinyint(4) default NULL,
        `prong2_character` tinyint(4) default NULL,
        `prong3_character` tinyint(4) default NULL,
        `gamma_conversions` tinyint(4) default NULL,
        `vee_vertices` tinyint(4) default NULL,
        `neutrons` tinyint(4) default NULL,
        `notes` text,
        `modification_date` datetime default NULL,
        `filename` varchar(100) default NULL,
        `idhits` int(11) default NULL,
        `odhits` int(11) default NULL,
        `tmean` float default NULL,
        `calpe` float default NULL
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

#DROP TABLE IF EXISTS `scanner_inbox`;
CREATE TABLE `scanner_inbox` (
        `id` int(11) NOT NULL auto_increment,
        `user_name` char(30) default NULL,
        `det` char(30) default 'TP',
        `recoVer` char(10) default 'v6r2p1',
        `run` int(11) default NULL,
        `subrun` int(11) default NULL,
        `gate` int(11) default NULL,
        `slice` int(11) default NULL,
        PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;


#DROP TABLE IF EXISTS `manreco`;
CREATE TABLE `manreco` (
  `user_name` char(30) default NULL,
  `det` char(10) default NULL,
  `recoVer` char(10) default NULL,
  `run` int(11) default NULL,
  `subrun` int(11) default NULL,
  `gate` int(11) default NULL,
  `slice` int(11) default NULL,
  `modification_date` datetime default NULL,
  `filename` char(100) default NULL,
  `recodata` text default NULL
) ENGINE=MyISAM DEFAULT CHARSET=latin1;
